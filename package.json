{
  "name": "react-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode dev --open",
    "build": "tsc && vite build",
    "build:prod": "tsc && vite build --mode prod",
    "build:test": "tsc && vite build --mode test",
    "build:dev": "tsc && vite build --mode dev",
    "preview": "vite preview",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\"",
    "lint:style": "stylelint \"**/*.css\"",
    "lint:js:fix": "eslint --ext .js,.jsx,.ts,.tsx ./src --fix",
    "lint:style:fix": "stylelint \"**/*.css\" --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.css": "stylelint",
    "**/*.{js,jsx,ts,tsx}": "eslint --ext .js,.jsx,.ts,.tsx",
    "**/*.{js,jsx,tsx,ts,css,md,json}": "prettier --ignore-unknown --write"
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^3.4.0",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.2",
    "@types/classnames": "^2.3.1",
    "ahooks": "^3.7.5",
    "craco-less": "2.0.0",
    "crypto-js": "^4.1.1",
    "less-loader": "^11.1.0",
    "moment": "^2.29.4",
    "npm": "^9.6.0",
    "number-precision": "^1.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "terser": "^5.16.5",
    "ua-device": "^0.1.10"
  },
  "devDependencies": {
    "@locator/babel-jsx": "^0.2.4",
    "@types/node": "^18.11.10",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^2.2.0",
    "antd": "^5.0.1",
    "axios": "^1.2.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "less": "^4.1.3",
    "path": "^0.12.7",
    "prettier": "^2.8.4",
    "react-router-dom": "^6.4.3",
    "stylelint": "^15.3.0",
    "stylelint-config-standard": "^31.0.0",
    "stylelint-prettier": "^3.0.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  }
}
